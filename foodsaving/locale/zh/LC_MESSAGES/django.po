#, python-format
msgid "Thank you for signing up to %(site_name)s!"
msgstr "感謝你在%(site_name)s註冊"

#: foodsaving/users/templates/mailverification.html.jinja2:108
#, python-format
msgid "Welcome to %(site_name)s, %(user_display_name)s!"
